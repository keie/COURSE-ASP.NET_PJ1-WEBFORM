        //    }, 10);
        //});

    },
    stickMenus: function () {
        if (CSI.UIActions.StickyMenu.isInitialised) {
            $.each($('[data-sticky-menu]'), function (e, f) {

                var trueOffset = $(f).data('trueOffset');

                if (trueOffset == undefined) {
                    trueOffset = $(f).offset().top - $(f).data('fixedHeight');
                    $(f).data('trueOffset', trueOffset);
                }

                if ($(window).scrollTop() >= trueOffset) {
                    $(f).css('width', $(f).outerWidth());
                    $(f).css('top', $(f).data('fixedHeight'));
                    $(f).addClass('fixed');

                    var next = CSI.Utilities.findNextInDom($(f));
                    $(next).css('margin-top', $(f).outerHeight(true));
                } else {
                    $(f).removeAttr('style');
                    $(f).removeClass('fixed');
                    var next = CSI.Utilities.findNextInDom($(f));
                    $(next).removeAttr('style');
                }
            });
        }
    }
});
