CSI.UIActions.extend('navigationGrower', {
    init: function () {
        if ($('[data-navgrow-wrapper]').length > 0) {
            $(window).bind('resizeEnd', function () {
                var headerHeight = $('.header-middle').outerHeight(true);
                $('.pop-search').height(headerHeight);
            });
        }
        if ($('[data-overflow-wrapper]').length > 0) {
            $(window).bind('resizeEnd', function () {
                CSI.UIActions.navigationGrower.handleOverFlow();
            });
        }
        $(window).trigger('resizeEnd');
    },
    handleNav: function () {
        var noneMenuWidth = 0;
        var wrapperWidth = $('[data-navgrow-wrapper]').outerWidth(true);
        var navWidth = $('[data-navgrow-nav]').outerWidth(true);

        $.each($('[data-navgrow-nonemenu]'), function (e, f) {
            noneMenuWidth += $(f).outerWidth(true);
        });

        var remainingSpace = wrapperWidth - noneMenuWidth;

        if (navWidth < remainingSpace) {
            $('[data-navgrow-wrapper]').addClass('slim');
        } else {
            $('[data-navgrow-wrapper]').removeClass('slim');
        }

        $('main').css('padding-top', $('.header').outerHeight(true));
    },
    handleOverFlow: function () {
        var noneMenuWidth = 0;
        var wrapperWidth = $('[data-overflow-wrapper]').outerWidth(true);
        var navWidth = $('[data-overflow-nav]').outerWidth(true);
        var iconWidth = $('[data-overflow-nav-icon]').outerWidth(true);

        navWidth = navWidth - iconWidth;

        $.each($('[data-overflow-nonemenu]'), function (e, f) {
            noneMenuWidth += $(f).outerWidth(true);
        });

        var remainingSpace = wrapperWidth - noneMenuWidth - iconWidth;

        $.each($('[data-overflow-nav] > li:not(".overflow-area")'), function (e, f) {
            var elementwidth = $(f).outerWidth(true) + 1;
            remainingSpace = remainingSpace - elementwidth;

            if (remainingSpace <= 0) {
                $(f).hide();
                var link = $(f).find('a').first();

                var textToFind = $.trim(link.text());

                if ($('[data-overflow-nav-holder] a:contains("' + textToFind + '")').size == 0) {
                    $('[data-overflow-nav-holder]').prepend(CSI.UIActions.navigationGrower.convertToLink(link));
                }
            }
            else {
                $(f).show();
                var textToFind = $.trim($(f).text());
                $('[data-overflow-nav-holder] a:contains("' + textToFind + '")').remove();
            }
        });
    },
    convertToLink: function (element) {
        return '<a rel="nofollow" href="' + $(element).attr('href') + '">' + $(element).text() + '</a>';
    }
});
