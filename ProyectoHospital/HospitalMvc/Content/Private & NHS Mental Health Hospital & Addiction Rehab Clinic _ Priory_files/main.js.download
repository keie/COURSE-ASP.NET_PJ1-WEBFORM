function calcAccordion(iAccordionNumber) {

    $('.accordion-' + iAccordionNumber + ' .expander').removeClass('active');//.removeClass('visible');
    $('.accordion-' + iAccordionNumber + ' .tab-wrap').removeClass('set');
    $('.accordion-' + iAccordionNumber + ' .tab-nav .tab').removeClass('overflow').removeClass('open').removeAttr('style');

    if ($('.mob-detect').is(':hidden') || $('.detect-xs').is(':hidden')) {

        setTimeout(function () {

            $('.accordion-' + iAccordionNumber + ' .expander').addClass('visible');
            //$('.accordion-' + iAccordionNumber + ' .tab-nav .tab.overflow');

            var iTabLength = 0;
            var iHeightOffset = 0;
            var iWidestWidth = 0;
            var iCurrentWidth = 0;
            var iVisibleNavWidth = 0;
            var iTabLengthWithExpander = 0;
            $('.accordion-' + iAccordionNumber + ' .tab-wrap').css('width', '');
            var iTabArea = $('.accordion-' + iAccordionNumber + ' .tab-wrap').outerWidth(true);
            var iExpanderWidth = $('.accordion-' + iAccordionNumber + ' .expander').outerWidth(true);

            $('.accordion-' + iAccordionNumber + ' .tab-nav .tab').each(function () {

                that = $(this);

                iTabLength = (iTabLength + Math.floor(that.outerWidth(true)));
                iTabLengthWithExpander = (iTabLength + iExpanderWidth);
                if (iTabLengthWithExpander > iTabArea) {
                    if (iVisibleNavWidth == 0) {
                        iVisibleNavWidth = (iTabLength - that.outerWidth(true));// + 1);
                    }
                    iHeightOffset = (iHeightOffset + Math.floor(that.outerHeight()));// - 1);
                    iCurrentWidth = that.outerWidth(true)
                    if (iCurrentWidth > iWidestWidth) {
                        iWidestWidth = iCurrentWidth;
                    }
                    that.addClass('overflow');
                    that.css('top', iHeightOffset);
                };
            });

            if ($('.accordion-' + iAccordionNumber + ' .tab-nav .tab.overflow').length > 0) {
                $('.accordion-' + iAccordionNumber + ' .tab-nav .tab.overflow').css('width', iWidestWidth);
                //$('.accordion-' + iAccordionNumber + ' .tab-nav .tab-wrap').css('width', iVisibleNavWidth);
                $('.accordion-' + iAccordionNumber + ' .expander').addClass('visible');
            } else {
                $('.accordion-' + iAccordionNumber + ' .expander').removeClass('visible');
                $('.accordion-' + iAccordionNumber + ' .tab-wrap').addClass('no-expander');
            }
            $('.accordion-' + iAccordionNumber + ' .tab-wrap').addClass('set');
        }, 250);

    }

}

$(function () {

