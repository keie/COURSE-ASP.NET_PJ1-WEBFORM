CSI.extend("Utilities", {
    init: function () {
        CSI.Utilities.gridslider.init();
    },
    getQueryStringValue: function (name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    },
    findNextInDom: function (elm) {
        if (elm.next().length > 0) {
            return elm.next()[0];
        } else {
            return CSI.Utilities.findNextInDom(elm.parent());
        }
    }
});
