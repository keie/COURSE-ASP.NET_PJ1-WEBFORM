CSI.UIActions.extend('TransitionGrower', {
    init: function () {
        CSI.UIActions.TransitionGrower.getHeights();

        $('[data-transition-grow-trigger]').on('click', function (e) {
            CSI.UIActions.TransitionGrower.bindHeights(undefined, undefined, e);
        });
    },
    getHeights: function () {
        $.each($('[data-transition-grow]'), function (e, f) {
            var allHeight = 0;
            var initialHeight = 0;
            var initCount = $(f).data('transition-initcount');

            var selector = $(f).data('transition-selector');

            $.each($(selector, f), function (e, f) {
                allHeight += $(f).outerHeight(true);
            });

            $.each($(selector + ':lt(' + initCount + ')'), function (e, f) {
                initialHeight += $(f).outerHeight(true);
            });

            $(f).data('all-height', allHeight);
            $(f).data('initial-height', initialHeight);

            CSI.UIActions.TransitionGrower.bindHeights('initial-height', $(f));
        });
    },
    bindHeights: function (type, elm, event) {
        if (elm === undefined) {
            elm = $($(event.target).closest('[data-transition-section]').data('transition-section'));
        }

        var activeType = elm.data('height-type');

        if (type === undefined && activeType === "all-height") {
            type = 'initial-height';
        } else if (type === undefined) {
            type = "all-height";
        }

        elm.data('height-type', type);

        elm.css('height', elm.data(type));
    }
});
CSI.UIActions.extend('GoogleTagManagerForms', {
    init: function () {

        $('div[id^=contour_form_] form').on('submit', function (e) {

            if ($('.field-validation-error', this).length > 0)
            {
                return;
            }

            $('.btn', this).attr("disabled", "disabled");

            var dl = window.dataLayer || [];

            var submission = {
                'formName': $('#FormName').val()
            };

            try
            {
                $('[data-ga-var]', this).each(function () {
                    if ($(this).attr('type') === 'checkbox' || $(this).attr('type') === 'radio') {
                        submission[$(this).data('gaVar')] = $(this).prop('checked');
                    } else {
                        var regExpr = /[^a-zA-Z0-9-. @]/g;
                        var userText = $(this).val() === '' ? undefined : $(this).val();
                        if (userText !== undefined) {
                            userText = userText.replace(regExpr, "");
                        }
                        submissionName = $(this).data('gaVar');
                        submissionName = submissionName.replace(regExpr, "");

                        submission[submissionName] = userText;
                    }
                });
            }
            catch (err) {
                $.ajax({
                    url: "/umbraco/api/JSErrorHandlerAPI/RecordError?message=" + err.message,
                    async: true,
                    success: function (data) {

                    }
                });
            }

            submission['event'] ='form-submitted';

            var nodeId = $('#NodeId').val();
            $.ajax({
                url: "/umbraco/api/FormTrackingAPI/TrackFormPost?pageId=" + nodeId + "&json=" + JSON.stringify(submission),
                async: true,
                success: function (data) {

                }
            });

            dl.push(submission);

        });
    }
});
