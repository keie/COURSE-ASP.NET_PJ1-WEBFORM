CSI.UIActions.extend('StickyMenu', {
    isInitialised: false,
    init: function () {
        $.each($('[data-sticky-menu]'), function (e, f) {
            var contactElm = $($(f).data('contact-element'));
            if (contactElm.css("position") == "fixed" || contactElm.css('position') == 'static') {
                $(f).data('fixedHeight', contactElm.outerHeight(true));
            }
            else {
                $(f).data('fixedHeight', 0);
            }
        });
        CSI.UIActions.StickyMenu.isInitialised = true;
        $(window).scroll(function (event) {
            if (this.scrollTO) clearTimeout(this.scrollTO);
            this.scrollTO = setTimeout(function () {
                CSI.UIActions.StickyMenu.stickMenus();
            }, 10);
        });

        //$(window).resize(function () {
        //    if (this.resizeTO) clearTimeout(this.resizeTO);
