    ['dependentproperty', 'desiredvalue'],
    function (options) {
        options.rules['requiredif'] = {
            dependentproperty: options.params['dependentproperty'],
            desiredvalue: options.params['desiredvalue']
        };
        options.messages['requiredif'] = "computer says no";
    });