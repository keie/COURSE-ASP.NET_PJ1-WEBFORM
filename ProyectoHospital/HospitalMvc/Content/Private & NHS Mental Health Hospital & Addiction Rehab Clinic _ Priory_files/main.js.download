function ExecuteIfNotNull(a, b, c) {
    var prefix = '';
    var d = a.split('.');

    var g = false;

    $.each(d, function (e, f) {
        prefix += f;
        if (typeof (eval(prefix)) == 'object') {
            g = true;
        }
        else {
            g = false;
            return false;
        }
        prefix += '.';
    });
    if (g) {
        if (typeof (eval(prefix + b)) == 'function') {
            eval(prefix + b)(c);
        }
    }
}

