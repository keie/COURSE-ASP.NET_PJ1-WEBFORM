CSI.UIActions.extend('PopOverSelects', {
    init: function () {
        $('[data-popover-value]').change(function (e) {
            CSI.UIActions.PopOverSelects.checkboxClick(e.target);
        });

        $('[data-target-popover] .form-control').on('click', function (e) {
            var elm = $(e.target).parent();
            var container = CSI.UIActions.PopOverSelects.getPopOverContainer(elm.data('target-popover'));

            if (container.hasClass('open')) {
                CSI.UIActions.PopOverSelects.closePopOver(container);
            } else {
                CSI.UIActions.PopOverSelects.openPopOver(container, elm.parent());
            }

        });
    },
    getPopOverContainer: function (name) {
        return $('[data-popover-name="' + name + '"]');
    },
    openPopOver: function (container, topLevelControl) {
        container.data('changed', false);
        //container.slideToggle(0).addClass('open');
        container.slideToggle().addClass('open').parent().addClass("open");
        setTimeout(function () {
            container.css('overflow', 'auto');
        }, 400);
        $(document).on('click', function (e) {
            if (!topLevelControl.is(e.target) && topLevelControl.has(e.target).length === 0) {
                CSI.UIActions.PopOverSelects.closePopOver(container);
                //console.log('triggered mouseup close');
            }
        });

    },
    closePopOver: function (container) {
        //container.slideToggle(0).removeClass('open');
        container.slideToggle().removeClass('open').parent().removeClass("open");
        var sTargetPopover = container.data('popover-name');
        var topLevelControl = $('[data-target-popover=' + sTargetPopover + ']');
        if (container.data('changed')) {
            topLevelControl.find('.form-control-value').trigger('change');
        }
        $(document).off('click');
        //console.log('closePopOver - closed');
    },
    checkboxClick: function (e) {
        var parent = $(e).closest('[data-popover-name]');
        $(parent).data('changed', true);
        var sTargetPopover = parent.data('popover-name');

        var topLevelControl = $('[data-target-popover=' + sTargetPopover + ']');

        var mode = topLevelControl.data('select-mode') == undefined ? 'multi' : topLevelControl.data('select-mode');

        if (mode == 'multi') {
            CSI.UIActions.PopOverSelects.checkboxMultiHandler(parent, topLevelControl);
        } else {
            CSI.UIActions.PopOverSelects.checkboxSingleHandler(parent, topLevelControl, e);
        }
    },
    checkboxMultiHandler: function (parent, topLevelControl) {
        var sDelim = '';
        var sPostDelim = '';
        var sSelectableList = '';
        var sPostValue = '';

        parent.find('[data-popover-value]:checked').each(function () {
            sSelectableList = (sSelectableList + sDelim + $(this).val());
            sPostValue = (sPostValue + sPostDelim + $(this).data('post-value'));
            sDelim = ', ';
            sPostDelim = ','
        });

        topLevelControl.find('.form-control').val(sSelectableList).prop('title', sSelectableList);
        topLevelControl.find('.form-control-value').val(sPostValue);
    },
    checkboxSingleHandler: function (parent, topLevelControl, e) {
        parent.find('[data-popover-value]:checked').not(e).each(function (e, f) {
            $(f).prop('checked', false);
        });

        topLevelControl.find('.form-control').val($(e).val()).prop('title', $(e).val());
        topLevelControl.find('.form-control-value').val($(e).data('post-value'));
        if ($(e).data("url")) {
            window.location.href = $(e).data("url");
        }
        CSI.UIActions.PopOverSelects.closePopOver(parent);
        $(document).off('mouseup');
    },
    removeHidden: function (wrapper) {
        var parent = $(wrapper).children('[data-popover-name]');

        var sTargetPopover = $(parent).data('popover-name');

        var topLevelControl = $('[data-target-popover=' + sTargetPopover + ']');

        parent.find('[data-popover-value]:checked').each(function (e, f) {
            if ($(f).hasClass('noshow')) {
                $(f).prop('checked', false);
            }
        });

        CSI.UIActions.PopOverSelects.checkboxMultiHandler(parent, topLevelControl);

    }
});
