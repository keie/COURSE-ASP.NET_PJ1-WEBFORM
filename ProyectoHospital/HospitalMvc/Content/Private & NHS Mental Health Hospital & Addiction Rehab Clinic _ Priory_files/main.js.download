    switchViewMode: function (e) {
        $('.btn-filter').removeClass('active');
        var tabToView = $(e).data('view');
        $(e).addClass('active');

        switch (tabToView) {
            case 'list':
            case 'grid':
                $('.map-wrap').hide();
                $('.destination-wrap').show();
                $('.destination-wrap').removeClass('grid');
                $('.destination-wrap').removeClass('list');
                $('.destination-wrap').addClass(tabToView);
                break;
            case 'map':
                $('.destination-wrap').hide();
                $('.map-wrap').show();
                break;
        }
    },
    filterResults: function () {
        var destinationFilter = $('[data-plugin="destination-ajax-filter"]').val();
        var resortFilter = $('[data-plugin="resort-ajax-filter"]').val();
        var startDateFilter = $('[data-plugin="startdate-ajax-filter"]').val();
        var occupancyFilter = $('[data-plugin="occupancy-ajax-filter"]').val();

        var categoryFilter = $('[data-plugin="category-ajax-filter"]').val();
        var archiveFilter = $('[data-plugin="archive-ajax-filter"]').val();
        var keywordFilter = $('[data-plugin="keyword-ajax-filter"]').val();

        $.each($('[data-plugin="filteritem"]'), function (e, f) {
            var valid = true;

            if (resortFilter != undefined) {
                if ($(f).data('resort') != resortFilter && resortFilter != '-1') {
                    valid = false;
                }
            }

            if (destinationFilter != undefined) {
                if ((valid && $(f).data('availability').indexOf(startDateFilter) == -1) && startDateFilter != '-1') {
                    valid = false;
                }
            }

            if (occupancyFilter != undefined) {
                if ((valid && $(f).data('occupancy').indexOf(occupancyFilter) == -1) && occupancyFilter != '-1') {
                    valid = false;
                }
            }

            if (categoryFilter != undefined) {
                if ((valid && $(f).data('category').indexOf(categoryFilter) == -1) && categoryFilter != '-1') {
                    valid = false;
                }
            }

            if (archiveFilter != undefined) {
                if ((valid && $(f).data('date').indexOf(archiveFilter) == -1) && archiveFilter != '-1') {
                    valid = false;
                }
            }

            if (keywordFilter != undefined) {
                if ((valid && $(f).filter(':contains("' + keywordFilter + '")').size() == 0) && keywordFilter != '') {
                    valid = false;
                }
            }

            $(f).data('valid', valid);
        });

        CSI.filters.calculatePageSize();
    },
    calculatePageSize: function () {
        if ($('[data-plugin="pagesize"]').size() > 0) {
            var pagesize = $('[data-plugin="pagesize"]').val();

            var resultsCount = $('[data-plugin="filteritem"]').filter(function () { return $(this).data('valid') == true }).size()

            var totalPages = Math.ceil(resultsCount / pagesize);

            totalPages = totalPages == 0 ? 1 : totalPages;

            $('.page-of').text('of ' + totalPages);
            $('.page-of').data('maxpage', totalPages);

            var pagingControl = $('.paging');

            $('.item', pagingControl).remove();

            var element = $('.paging');

            if (pagingControl.data("pagingstyle") == "list") {
                element.empty();
                var prefix = 'Page';

                if (element.attr('data-prefix') != undefined) {
                    prefix = element.data('prefix');
                }

                for (var i = 1; i <= totalPages; i++) {
                    element.append('<option value="' + i + '">' + prefix + ' ' + i + '</option>');
                }

                if (element.data('pagingviewall') == true) {
                    element.append('<option value="-1">View All</option>');
                }

                element.on('change', function () {
                    CSI.filters.movePage(parseInt($(this).val()));
                });
            }
            else {
                for (var i = 1; i <= totalPages; i++) {
                    var active = i == 1 ? "item active" : "item";
                    $('.paging li.page-of').before('<li class="' + active + '"><a href="javascript:void(0);">' + i + '</a></li>');
                }
            }

            $('.paging li.item').on('click', function () {
                CSI.filters.movePage(parseInt($(this).text()));
            });

            CSI.filters.movePage(1);

            if (resultsCount == 0 || resultsCount === undefined) {
                $('.no-results, [data-noresults], [data-resetmessage]').removeClass('hidden');
                $('.sub-title').addClass('hidden');
            }
            else {
                $('.no-results, [data-noresults], [data-resetmessage]').addClass('hidden');
                $('.sub-title').removeClass('hidden');
            }

            if ($('.title-count').size() > 0) {
                $('.title-count').text(resultsCount);
                var slider = $('[data-pricefilter]');
                if (slider.length > 0) {
                    var currency = slider.data('currency');
                    var min = CSI.filters.getminPrice();
                    $('.title-bar .title-price').html(currency + min);
                }
            }
        }
        $.each($('[data-plugin="filteritem"]'), function (e, f) {
            var mapLink = $('[data-maptrigger]', f);

            if (mapLink) {
                mapLink.off('click').on('click', function (e, f) {
                    CSI.filters.switchViewMode($('[data-view="map"]'));
                    CSI.filters.addmapview();

                    var parent = $(e.target).closest('[data-plugin="filteritem"]');

                    CSI.Maps.Latitude = parent.data('pointlatitude');
                    CSI.Maps.Longitude = parent.data('pointlongitude');

                    CSI.Maps.RecentreMap();

                    $.each(CSI.Maps.Markers, function (e, f) {
                        if (f.key == parent.data('pointlatitude') + '-' + parent.data('pointlongitude')) {
                            var content = parent.data('mapcontent') == '' ? $('.h-other', f).text() : parent.data('mapcontent');
                            CSI.Maps.ContentWindow = new google.maps.InfoWindow({ content: content });
                            CSI.Maps.ContentWindow.open(CSI.Maps.MapObject, f.marker);
                        }
                    });
                });
            }
        });
    },
    movePage: function (pageNumber) {
        $('.paging li.item').removeClass('active');
        $('.paging li.item:contains("' + pageNumber + '")').addClass('active');

        if (pageNumber !== -1) {
            var pageSize = parseInt($('[data-plugin="pagesize"]').val());

            var startIndex = pageNumber == 1 ? 0 : (pageSize * (pageNumber - 1));

            $('[data-plugin="filteritem"]').addClass('hidden');

            $.each($('[data-plugin="filteritem"]').filter(function () { return $(this).data('valid') == true }).slice(startIndex, startIndex + pageSize), function (e, f) {
                $(f).removeClass('hidden');
            });
        }
        else {
            $.each($('[data-plugin="filteritem"]').filter(function () { return $(this).data('valid') == true }), function (e, f) {
                $(f).removeClass('hidden');
            });
        }
    },
    sortResults: function (val) {
        var items = $('[data-plugin="filteritem"]');

        switch (val) {
            case 'price':
                items.sort(SortByPrice);
                break;
            case '-price':
                items.sort(SortByPriceDesc);
                break;
            case 'name':
                items.sort(SortByName);
                break;
            case '-name':
                items.sort(SortByNameDesc);
                break;
            case 'location':
                items.sort(SortByLocation);
                break;
            case '-location':
                items.sort(SortByLocationDesc);
                break;
        }

        $(".tbl-d").append(items);
        CSI.filters.movePage(1);
    },
    addmapview: function () {
        if (typeof CSI.MapsV2 != "object") {
            var mapContainer = $('.map');

            var scrollable = mapContainer.data('scollable') === undefined ? false : mapContainer.data('scollable');

            if (mapContainer.data("donotempty") === "undefined") {
                mapContainer.empty();
            }

            var zoom = mapContainer.data('mapzoom') == '' ? 8 : mapContainer.data('mapzoom');

            var mapMarker = mapContainer.data("marker");

            CSI.Maps.Zoom = zoom;
            CSI.Maps.Latitude = mapContainer.data('centerlatitude');
            CSI.Maps.Longitude = mapContainer.data('centerlongitude');
            CSI.Maps.MapType = 'Road';
            CSI.Maps.SchoolWheel = scrollable === undefined ? false : scrollable;;
            var map = CSI.Maps.CreateMap(document.getElementById("destination-map"));

            $.each($('[data-plugin="filteritem"]').not('.hidden'), function (e, f) {
                var latitude = $(f).data("pointlatitude");
                var longitude = $(f).data("pointlongitude");

                var marker = CSI.Maps.AddMarkerMultiple(false, latitude, longitude);
                if (mapMarker != '') {
                    marker.setIcon(mapMarker);
                }
                $(marker).data('filtername', $(f).data('name'));

                var link = $(f).data('maplink') == '' ? $(f).attr('href') : $(f).data('maplink');

                var content = $(f).data('mapcontent') == '' ? $('.h-other', f).text() : $(f).data('mapcontent');

                google.maps.event.addListener(marker, 'click', function () {
                    if (CSI.Maps.ContentWindow) {
                        CSI.Maps.ContentWindow.close();
                    }
                    CSI.Maps.ContentWindow = new google.maps.InfoWindow({ content: content });
                    CSI.Maps.ContentWindow.open(map, marker);
                });
            });
        }
    },
    buildfilteroption: function (type, f, filter) {
        if (type == 'list') {
            return '<label><div class="flex-wrapper"><input type="checkbox" data-filtertype="' + filter + '" value="' + f.value + '"><div class="pseudo-checkbox"><span class="fa fa-check"></span></div><span class="amount">' + f.value + ' (' + f.count + ')</span></div></label>';
        }
    },
    buildNoFilterOptions: function (type) {
        if (type == 'list') {
            return '<label><div class="flex-wrapper"<span class="amount">No filter options available</span></div></label>';
        }
    },
    calculateFilterArray: function (b, c) {
        var values = [];
        $.each($('[data-plugin="filteritem"]'), function (e, f) {
            var dataAmount = $(f).data(b);

            if (dataAmount !== '') {

                if (typeof (dataAmount) === "string" && dataAmount.indexOf(',') > -1) {
                    var splitDataAmount = dataAmount.split(',');
                    $.each(splitDataAmount, function (e, f) {
                        values = CSI.filters.increaseoraddtofilterarray(values, f);
                    });
                }
                else {
                    values = CSI.filters.increaseoraddtofilterarray(values, dataAmount);
                }
            }
        });

        var sortBy = function (a, b) {
            return (a.value - b.value);
        }

        values = values.sort(sortBy);

        $.each(values, function (e, f) {
            c.append(CSI.filters.buildfilteroption('list', f, b));
        });

        if (values.length == 0) {
            c.append(CSI.filters.buildNoFilterOptions('list'));
        }
    },
    increaseoraddtofilterarray: function (values, dataAmount) {
        if (values.length > 0) {
            var matched = false;
            $.each(values, function (e, f) {
                if (f.value == dataAmount) {
                    f.count++;
                    matched = true;
                }
            });
            if (!matched) {
                values.push({ value: dataAmount, count: 1 });
            }
        }
        else {
            values.push({ value: dataAmount, count: 1 });
        }
        return values;
    },
    pricefilterinitialised: false,
    initialisePriceFilter: function () {
        if (!CSI.filters.pricefilterinitialised) {
            var min = CSI.filters.getminPrice();
            var max = CSI.filters.getmaxPrice();

            CSI.Searchbox.initaliseSlider(min, max);
            CSI.filters.pricefilterinitialised = true;
        }
    },
    getminPrice: function () {
        var min = 99999999;
        var items = CSI.filters.getcurrentvalidresults();
        $.each(items, function (e, f) {
            var price = parseInt($(f).data('price'));

            if (price < min && price !== 0) {
                min = price
            }
        });

        ///No results, make sure 99999999 is not displayed;
        if (min == 99999999) {
            min = 0;
        }
        return min;
    },
    getmaxPrice: function () {
        var max = 0;
        var items = CSI.filters.getcurrentvalidresults();
        $.each(items, function (e, f) {
            var price = parseInt($(f).data('price'));

            if (price > max) {
                max = price
            }
        });
        return max;
    },
    locationfilterinitialised: false,
    initialseLocationFilter: function () {
        if (!CSI.filters.locationfilterinitialised) {
            CSI.filters.calculateFilterArray('location', $('[data-filtercontroltarget="location"]'));
