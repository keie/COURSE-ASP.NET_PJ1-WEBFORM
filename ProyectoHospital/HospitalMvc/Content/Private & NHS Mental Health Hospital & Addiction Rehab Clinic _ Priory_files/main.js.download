    newfilterresults: function () {
        var slider = $('[data-slider-min]');
        var sliderValue = slider.data('slider').getValue();

        var validResults = CSI.filters.getresultvalidresults();

        $.each(validResults, function (e, f) {
            var price = $(f).data('price');

            var valid = price >= sliderValue[0] && price <= sliderValue[1];

            $(f).data('valid', valid);
        });


        CSI.filters.filterbytarget('location');
        CSI.filters.filterbytarget('beds');
        CSI.filters.filterbytarget('searchradius');
        CSI.filters.filterbytarget('walking');
        CSI.filters.filterbytarget('facilities');
        CSI.filters.filterbytarget('suitability');
        CSI.filters.filterbytarget('producttype');

        CSI.filters.calculatePageSize();
    },
    filterbytarget: function (target) {

        var validbytype = 'validby' + target;

        validResults = CSI.filters.getcurrentvalidresults();

        var filterElements = $('[data-filtercontroltarget="' + target + '"] input[type="checkbox"]:checked');

        if (filterElements.size() != 0) {
            $.each(validResults, function (e, f) {
                $(f).data(validbytype, false);
            });
        }
        else {
            $.each(validResults, function (e, f) {
                $(f).data(validbytype, true);
            });
        }

        $.each(filterElements, function (e, f) {
            var value = $(f).val();

            $.each(validResults, function (a, b) {
                var itemData = $(b).data(target);

                if (!$(b).data(validbytype)) {
                    var itemIsValid = (itemData + '').indexOf(value) != -1;
                    $(b).data(validbytype, itemIsValid);
                }
            });
        });

        $.each(validResults, function (e, f) {
            $(f).data('valid', $(f).data(validbytype));
        });
    },
    getcurrentvalidresults: function () {
        return $('[data-plugin="filteritem"]').filter(function () {
            return $(this).data('valid');
        });
    },
    getresultvalidresults: function () {
        var all = $('[data-plugin="filteritem"]');
        $.each(all, function (e, f) {
            $(f).data('valid', true);
        });

        return all;
    },
    filterByHtmlText: function (text) {
        validResults = CSI.filters.getresultvalidresults();

        if (text != '') {
            $.each(validResults, function (e, f) {
                $(f).data('valid', $(f).html().toLowerCase().indexOf(text) > -1);
            });
        }

        CSI.filters.calculatePageSize();
    },
    serializePostData: function () {
        location.search = $('[data-paginationcontroll][value!=""]').serialize();
    },
    updateDisplayValue: function (e) {
        var me = $(e);

        var actualFilter = $(me.data('filter'));
        var value = me.data('presentation-filter');
        var currentValue = actualFilter.val();

        var currentValueArr = currentValue.split(',');

        if (jQuery.inArray(value + '', currentValueArr) > 0) {
            currentValueArr = $.grep(currentValueArr, function (e, f) {
                return e != (value + '');
            });
        }
        else {
            currentValueArr.push(value);
        }

        actualFilter.val(currentValueArr.join(','));

        actualFilter.trigger('change');
    }
});
