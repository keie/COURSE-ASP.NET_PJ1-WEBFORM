                cookieBox.show();
                setTimeout(
                    function () {
                        cookieBox.fadeOut();
                        Cookies.set(stateCookieName, 1, { path: '/', expires: 365 });
                    }, 7000);
            }

            cookieBox.find('.icon').click(function () {
                cookieBox.fadeOut();
                Cookies.set(stateCookieName, 1, { path: '/', expires: 365 });
            });

        };
    }


    $('input[readonly]').on('focus', function (ev) {
