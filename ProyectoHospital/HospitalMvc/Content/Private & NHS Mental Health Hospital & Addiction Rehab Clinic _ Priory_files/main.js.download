    suitableFilterInitialised: false,
    initialiseSuitableForFilter: function () {
        if (!CSI.filters.suitableFilterInitialised) {
            CSI.filters.calculateFilterArray('suitability', $('[data-filtercontroltarget="suitability"]'));
            CSI.filters.suitableFilterInitialised = true;
